{
  "name": "findthemasks",
  "description": "findthemasks",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">= 12.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/findthemasks/findthemasks.git"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "scripts": {
    "build": "npm run webpack",
    "dev": "NODE_ENV='development' nodemon --ignore 'client/' server.js",
    "lint": "npm run lint-client && npm run lint-node",
    "lint-client": "eslint 'client/**/*.js' --config 'client/.eslintrc.json'",
    "lint-node": "eslint '**/*.js' --ignore-pattern 'client/**/*'",
    "start": "node server.js",
    "webpack": "NODE_ENV='production' webpack -p --config webpack.config.js",
    "test-root": "jest  --verbose --testPathIgnorePatterns='/functions'",
    "test-functions": "jest --testPathPattern=./functions"
  },
  "dependencies": {
    "banana-i18n": "^1.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.3",
    "handlebars-helpers": "^0.10.0",
    "http-proxy-middleware": "^1.0.3",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-destructuring": "^7.10.1",
    "@babel/preset-env": "^7.9.5",
    "@google/markerclustererplus": "^5.0.3",
    "@googlemaps/js-api-loader": "^1.0.0",
    "babel-loader": "^8.1.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.5.2",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.8.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "fibers": "^4.0.2",
    "jest": "^26.1.0",
    "lodash.template": ">=4.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mobius1-selectr": "^2.4.13",
    "mocha": "^7.1.1",
    "nock": "^13.0.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.3",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "winston": "^3.2.1"
  }
}
